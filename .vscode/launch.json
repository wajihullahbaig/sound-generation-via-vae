    "version": "0.2.0",
    "configurations": [
        {
            "name": "Training debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "free_spoken_digit/main.py",            
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "%PYTHONPATH%;${workspaceFolder}"
            },
        },
        {
            "name": "Latent-to-Wav",
            "type": "debugpy",
            "request": "launch",
            "program": "free_spoken_digit/create_audios.py",            
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "%PYTHONPATH%;${workspaceFolder}"
            },
        },
    ]
}